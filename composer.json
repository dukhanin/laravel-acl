{
  "name": "dukhanin/laravel-acl",
  "description": "Laravel 5 ACL and Roles Bundle",
  "authors": [
    {
      "name": "Anton Dukhanin",
      "email": "anton.dukhanin@gmail.com"
    }
  ],
  "require": {
    "php": "~7.1",
    "laravel/framework": "~5.6",
    "dukhanin/laravel-support": "^2.0"
  },
  "require-dev": {
    "laravel/laravel": "~5.5",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Dukhanin\\Acl\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\": "tests/app/",
      "Dukhanin\\Acl\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Dukhanin\\Acl\\Providers\\AclServiceProvider"
      ]
    }
  }
}
